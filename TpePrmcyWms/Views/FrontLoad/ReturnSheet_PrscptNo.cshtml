@model List<TpePrmcyWms.Models.HsptlApiUnit.Data_IPDPatSeq>;
@{
    Layout = null;
}

@if (Model == null)
{
    <option value="">查詢錯誤</option>

}
else if(Model.Count == 0)
{
    <option value="">查無資料</option>
}
else
{
    @foreach (var item in Model)
    {
        <option value="@(item.PAT_SEQ)/@(item.BED_CODE)/@(item.REAL_OUT_DATE)">
            @(item.PAT_SEQ)/@(item.BED_CODE)/@(item.REAL_OUT_DATE)
        </option>
    }
}
