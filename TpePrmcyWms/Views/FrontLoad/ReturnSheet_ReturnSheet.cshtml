@model List<TpePrmcyWms.Models.HsptlApiUnit.Data_IPDReturn>;
@{
    Layout = null;
}

@if (Model == null)
{
    <option value="">查詢錯誤</option>

}
else if(Model.Count == 0)
{
    <option value="">查無資料</option>
}
else
{
    @foreach (var item in Model)
    {
        <option value="@(item.ODR_CODE)，@(item.ODR_NAME)，@(item.DRUGBAG_SEQ)">@(item.ODR_CODE)，@(item.ODR_NAME)，@(item.DRUGBAG_SEQ)</option>
    }
}
